{
  "mcpServers": {
    "claude-flow": {
      "command": "./cli.js",
      "args": [
        "mcp",
        "start",
        "--transport",
        "stdio",
        "--enterprise"
      ],
      "env": {
        "NODE_ENV": "production",
        "FLOWX_MCP_SERVER": "enterprise",
        "FLOWX_ENTERPRISE_FEATURES": "neural,swarm,memory,monitoring,workflow,github,filesystem,web,database,system",
        "FLOWX_NEURAL_WASM_ENABLED": "true",
        "FLOWX_TOOL_CATEGORIES": "neural,memory,monitoring,workflow,github,daa,system,flowx,swarm,filesystem,web,database",
        "FLOWX_AUTH_ENABLED": "false",
        "FLOWX_METRICS_ENABLED": "true",
        "FLOWX_AUDIT_LOGGING": "true",
        "FLOWX_RATE_LIMITING": "true",
        "FLOWX_CACHING_ENABLED": "true",
        "FLOWX_LOAD_BALANCER": "true",
        "FLOWX_SESSION_TIMEOUT": "3600000",
        "FLOWX_MAX_SESSIONS": "100",
        "FLOWX_TOOL_COUNT": "100",
        "FLOWX_VERSION": "8.1.0"
      }
    }
  },
  "tools": {
    "neural": {
      "count": 15,
      "wasmAccelerated": true,
      "tools": [
        "neural_train", "neural_predict", "neural_status", "neural_patterns",
        "model_load", "model_save", "pattern_recognize", "cognitive_analyze",
        "learning_adapt", "neural_compress", "ensemble_create", "transfer_learn",
        "neural_explain", "wasm_optimize", "inference_run"
      ]
    },
    "memory": {
      "count": 10,
      "persistent": true,
      "tools": [
        "memory_usage", "memory_backup", "memory_restore", "memory_compress",
        "memory_sync", "cache_manage", "state_snapshot", "context_restore",
        "memory_analytics", "memory_persist"
      ]
    },
    "monitoring": {
      "count": 13,
      "realTime": true,
      "tools": [
        "performance_report", "bottleneck_analyze", "token_usage", "benchmark_run",
        "metrics_collect", "trend_analysis", "cost_analysis", "quality_assess",
        "error_analysis", "usage_stats", "health_check", "swarm_monitor",
        "agent_metrics"
      ]
    },
    "workflow": {
      "count": 11,
      "visualDesigner": true,
      "tools": [
        "workflow_create", "workflow_execute", "automation_setup", "pipeline_create",
        "scheduler_manage", "trigger_setup", "workflow_template", "batch_process",
        "parallel_execute", "sparc_mode", "task_orchestrate"
      ]
    },
    "github": {
      "count": 8,
      "automation": true,
      "tools": [
        "github_repo_analyze", "github_pr_manage", "github_issue_track",
        "github_release_coord", "github_workflow_auto", "github_code_review",
        "github_sync_coord", "github_metrics"
      ]
    },
    "jira": {
      "count": 7,
      "integration": true,
      "tools": [
        "jira_sync_issues", "jira_create_issue", "jira_update_issue",
        "jira_webhook_handler", "jira_field_mapping", "jira_status_check",
        "jira_bulk_operations"
      ]
    },
    "confluence": {
      "count": 6,
      "documentation": true,
      "tools": [
        "confluence_sync_docs", "confluence_create_page", "confluence_update_page",
        "confluence_generate_docs", "confluence_template_render", "confluence_space_manage"
      ]
    },
    "daa": {
      "count": 8,
      "dynamicAgents": true,
      "tools": [
        "daa_agent_create", "daa_capability_match", "daa_resource_alloc",
        "daa_lifecycle_manage", "daa_communication", "daa_consensus",
        "daa_fault_tolerance", "daa_optimization"
      ]
    },
    "system": {
      "count": 6,
      "security": true,
      "tools": [
        "security_scan", "backup_create", "restore_system",
        "log_analysis", "diagnostic_run", "config_manage"
      ]
    }
  },
  "capabilities": {
    "protocolVersion": "2024.11.5",
    "totalTools": 100,
    "categories": 14,
    "wasmAcceleration": true,
    "neuralNetworks": true,
    "enterpriseFeatures": true,
    "visualWorkflowDesigner": true,
    "realTimeMonitoring": true,
    "githubIntegration": true,
    "jiraIntegration": true,
    "confluenceIntegration": true,
    "dynamicAgentArchitecture": true,
    "crossSessionMemory": true,
    "swarmCoordination": true,
    "performanceOptimized": true,
    "auditLogging": true,
    "rateLimiting": true,
    "loadBalancing": true,
    "caching": true,
    "authentication": "configurable",
    "authorization": "rbac"
  },
  "performance": {
    "speedImprovement": "2.8-4.4x",
    "maxConcurrentSessions": 100,
    "sessionTimeout": 3600000,
    "requestsPerSecond": 100,
    "circuitBreakerThreshold": 5,
    "healthCheckInterval": 30000
  }
}