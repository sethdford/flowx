{
  "mcpServers": {
    "claude-flow": {
      "command": "./cli.js",
      "args": [
        "mcp",
        "start",
        "--transport",
        "stdio"
      ],
      "env": {
        "CLAUDE_FLOW_MODE": "enterprise",
        "CLAUDE_FLOW_FEATURES": "neural,swarm,memory,monitoring"
      }
    }
  }
}