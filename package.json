{
  "name": "flowx",
  "version": "1.0.0",
  "description": "Advanced AI agent orchestration system - FlowX",
  "main": "cli.js",
  "bin": {
    "flowx": "./cli.js"
  },
  "scripts": {
    "dev": "tsx src/cli/main.ts",
    "build": "node esbuild.config.cjs",
    "build:ts": "tsc",
    "build:binary": "pkg dist/cli/main.js --targets node18-linux-x64,node18-macos-x64,node18-win-x64 --output bin/flowx",
    "build:simple": "npm run build:ts && pkg dist/cli/simple-cli.js --output bin/flowx-simple",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint src --ext .ts,.js",
    "format": "prettier --write src",
    "postinstall": "node scripts/install.js",
    "prepublishOnly": "node scripts/prepare-publish.js",
    "prepare-publish": "node scripts/prepare-publish.js",
    "benchmark": "node benchmark/bin/benchmark-cli.js run \"Benchmark simple task\"",
    "benchmark:test": "node benchmark/bin/benchmark-cli.js test",
    "benchmark:example": "node benchmark/examples/basic.js"
  },
  "keywords": [
    "flowx",
    "ai",
    "agent",
    "orchestration",
    "mcp",
    "workflow",
    "automation",
    "claude",
    "anthropic"
  ],
  "author": "Seth Ford",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sethdford/flowx.git"
  },
  "bugs": {
    "url": "https://github.com/sethdford/flowx/issues"
  },
  "homepage": "https://github.com/sethdford/flowx#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "cli.js",
    "bin/",
    "dist/",
    "src/",
    ".claude/",
    "benchmark/src/",
    "benchmark/simple-benchmark.js",
    "benchmark/bin/",
    "benchmark/examples/",
    "benchmark/README.md",
    "scripts/install.js",
    "scripts/swarm-blessed-ui.js",
    "scripts/swarm-simple.js",
    "scripts/swarm-example.sh",
    "README.md",
    "LICENSE",
    "SWARM_TTY_SOLUTION.md",
    "SWARM_VISIBILITY.md"
  ],
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.35",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^19.1.8",
    "better-sqlite3": "^11.10.0",
    "blessed": "^0.1.81",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "ink": "^6.0.1",
    "ink-big-text": "^2.0.0",
    "ink-box": "^1.0.0",
    "ink-gradient": "^3.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-table": "^3.1.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^9.2.12",
    "nanoid": "^5.0.4",
    "node-pty": "^1.0.0",
    "ora": "^7.0.1",
    "react": "^19.1.0",
    "sql.js": "^1.13.0",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101",
    "@types/blessed": "^0.1.25",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "esbuild": "^0.25.5",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "p-queue": "^8.1.0",
    "pkg": "^5.8.1",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "pkg": {
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ],
    "scripts": "dist/**/*.js",
    "outputPath": "bin",
    "options": [
      "--experimental-specifier-resolution=node"
    ]
  },
  "type": "module"
}
